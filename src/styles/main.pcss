@import "open-props/postcss/style";
@import "themes.pcss";
@import url("https://fonts.googleapis.com/css2?family=Fredericka+the+Great&display=swap");

:where(*) {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

#nprogress {
  pointer-events: none;
}

#nprogress .bar {
  background: var(--color-brand);
  opacity: 0.75;
  position: fixed;
  z-index: 1031;
  top: 0;
  left: 0;
  width: 100%;
  height: 2px;
}
